{
  "ui": "tui",
  "tasks": {
    "test": {
      "dependsOn": ["^lint", "build"],
      "outputs": ["tests-coverage/**"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"],
      "cache": false
    },
    "dev:q": {
      "persistent": true,
      "dependsOn": [
        "^build"
      ]
    },
    "dev:p": {
      "persistent": true,
      "dependsOn": [
        "^build"
      ]
    },
    "dev:c": {
      "persistent": true,
      "dependsOn": [
        "^build"
      ]
    }
  }
}
