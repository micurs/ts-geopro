{
  "name": "@micurs/ts-geopro",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.umd.cjs"
    }
  },
  "scripts": {
    "dev": "vite build --watch",
    "build": "tsc && rimraf dist && vite build",
    "preview": "vite preview",
    "test": "vitest run --coverage",
    "watch:test": "vitest watch",
    "jsr:publish": "pnpm dlx jsr publish",
    "jsr:test-publish": "pnpm dlx jsr publish --dry-run --allow-dirty",
    "lint": "deno lint -c ./deno.lint.json --fix"
  },
  "devDependencies": {
    "@types/node": "^20.17.16",
    "@vitest/coverage-v8": "^3.0.4",
    "rimraf": "^5.0.10",
    "typescript": "^5.7.3",
    "vite": "^6.0.9",
    "vite-plugin-dts": "^4.5.0",
    "vitest": "^3.0.4"
  },
  "dependencies": {
    "gl-matrix": "^3.4.3"
  }
}
